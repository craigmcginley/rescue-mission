<h2>Questions!</h2>
  <%= link_to 'Submit a Question', new_question_path %>
<br>
<table>
  <tr>
    <th>Title</th>
    <th>Author</th>
    <th>Created At</th>
  </tr>
  <% @questions.each do |question| %>
    <tr>
      <td>
        <%= link_to question.title, question_path(question) %>
      </td>
      <td></td>
      <td><%= question.created_at %></td>
    </tr>
  <% end %>
</table>

